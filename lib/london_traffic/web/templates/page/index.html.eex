<div id="map">

</div>
<script>
  function initMap() {
    var valsFromElixir = <%= raw(@traffic_data) %>;
    var london = {
      "lat": 51.5074,
      "lng": 0.1278
    };
    var map = new google.maps.Map(document.getElementById('map'), {
      "zoom": 10,
      "center": london
    });

    valsFromElixir.forEach(function(pos) {
        new google.maps.Marker({
          "position": pos,
          "map": map
        });
      })
    }
</script>
